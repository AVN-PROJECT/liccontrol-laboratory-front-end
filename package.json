{
  "name": "package.json",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .js,.vue",
    "format": "prettier --write ."
  },
  "dependencies": {
    "stylelint-config-css-modules": "^4.4.0",
    "vite": "^6.3.5",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.0",
    "stylelint": "^16.0.2",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.12.1"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
